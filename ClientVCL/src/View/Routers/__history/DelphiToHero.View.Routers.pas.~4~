unit DelphiToHero.View.Routers;

interface

type
  TRouters = class
    private
    public
      constructor Create;
      destructor Destroy; Override;
  end;

var
  Router: TRouters;

implementation

uses
  Router4D;

{ TRouters }

constructor TRouters.Create;
begin

end;

destructor TRouters.Destroy;
begin

  inherited;
end;

initialization
  Router := TRouters.Create;

finalization
  Router.free;
end.
